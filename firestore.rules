rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Regra padrão: Bloqueia tudo por segurança.
    // Alterar para "allow read, write: if request.auth != null;" para permitir usuários logados.
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
